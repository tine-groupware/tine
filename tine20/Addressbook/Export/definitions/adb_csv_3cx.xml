<?xml version="1.0" encoding="UTF-8"?>
<config>
    <model>Addressbook_Model_Contact</model>
    <name>adb_csv_3cx</name>
    <!-- _('3CX CSV export') -->
    <label>3CX CSV export</label>
    <description>https://www.3cx.de/</description>
    <type>export</type>
    <plugin>Tinebase_Export_CsvNew</plugin>
    <icon_class>tinebase-action-export-csv</icon_class>
    <sort>
        <field>n_given</field>
        <direction>ASC</direction>
    </sort>
    <header>1</header>
    <delimiter>,</delimiter>
    <columns>
        <column>
            <identifier>n_given</identifier>
            <header>FirstName</header>
        </column>
        <column>
            <identifier>n_family</identifier>
            <header>LastName</header>
        </column>
        <column>
            <identifier>org_name</identifier>
            <header>Company</header>
        </column>
        <column>
            <identifier>tel_cell_normalized</identifier>
            <header>Mobile</header>
        </column>
        <column>
            <identifier>tel_cell_private_normalized</identifier>
            <header>Mobile2</header>
        </column>
        <column>
            <identifier>tel_home_normalized</identifier>
            <header>Home</header>
        </column>
        <column>
            <identifier></identifier>
            <header>Home2</header>
        </column>
        <column>
            <identifier>tel_work_normalized</identifier>
            <header>Business</header>
        </column>
        <column>
            <identifier></identifier>
            <header>Business2</header>
        </column>
        <column>
            <identifier>email</identifier>
            <header>Email</header>
        </column>
        <column>
            <identifier>tel_other_normalized</identifier>
            <header>Other</header>
        </column>
        <column>
            <identifier>tel_fax_home_normalized</identifier>
            <header>BusinessFax</header>
        </column>
        <column>
            <identifier>tel_fax</identifier>
            <header>HomeFax</header>
        </column>
        <column>
            <identifier>tel_pager</identifier>
            <header>Pager</header>
        </column>
    </columns>
</config>
