<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>adb_tine_import_csv</name>
    <model>Addressbook_Model_Contact</model>
    <plugin>Addressbook_Import_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>,</delimiter>
    <label>Tine contact CSV import"</label>
    <!-- _("Tine contact CSV import"") -->
    <description>Import CSV formated contacts from tine address book</description>
    <!-- _("Import CSV formated contacts from tine address book") -->
    <example>Addressbook/Import/examples/adb_tine_import.csv</example>
    <extension>csv</extension>
    <autotags>
        <tag>
            <name>Import list (###CURRENTDATE###)</name>
            <!-- _("Import list (###CURRENTDATE###)") -->
            <description>Contacts imported on ###CURRENTDATE### at ###CURRENTTIME### by ###USERFULLNAME###</description>
            <!-- _("Contacts imported on ###CURRENTDATE### at ###CURRENTTIME### by ###USERFULLNAME###") -->
            <contexts>
                <Addressbook />
            </contexts>
            <type>shared</type>
        </tag>
    </autotags>
