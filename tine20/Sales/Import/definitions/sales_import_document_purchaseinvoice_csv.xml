<?xml version="1.0" encoding="UTF-8"?>
<config>
    <name>sales_import_documentpurchaseinvoice_csv</name>
    <model>Sales_Model_Document_PurchaseInvoice</model>
    <plugin>Sales_Import_Document_PurchaseInvoice_Csv</plugin>
    <type>import</type>
    <headline>1</headline>
    <dryrun>0</dryrun>
    <delimiter>,</delimiter>
    <extension>csv</extension>
    <mapping>
        <field>
            <source>number</source>
            <destination>external_invoice_number</destination>
        </field>
        <field>
            <source>description</source>
            <destination>description</destination>
        </field>
        <field>
            <source>date</source>
            <destination>date</destination>
        </field>
        <field>
            <source>due_in</source>
            <destination>credit_term</destination>
        </field>
        <field>
            <source>payed_at</source>
            <destination>paid_at</destination>
        </field>
        <field>
            <source>payment_method</source>
            <destination>payment_means_used</destination>
        </field>
        <field>
            <source>discount</source>
            <destination>invoice_discount_sum</destination>
        </field>
        <field>
            <source>price_net</source>
            <destination>net_sum</destination>
        </field>
        <field>
            <source>sales_tax</source>
            <destination>sales_tax</destination>
        </field>
        <field>
            <source>due_at</source>
            <destination>due_at</destination>
        </field>
        <field>
            <source>price_gross</source>
            <destination>positions_gross_sum</destination>
        </field>
        <field>
            <source>price_tax</source>
            <destination>price_tax</destination>
        </field>
        <field>
            <source>price_total</source>
            <destination>gross_sum</destination>
        </field>
        <field>
            <source>supplier</source>
            <destination>supplier</destination>
        </field>
        <field>
            <source>costcenter</source>
            <destination>costcenter</destination>
        </field>
        <field>
            <source>contact</source>
            <destination>contact</destination>
        </field>
    </mapping>
</config>

