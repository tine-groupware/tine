    "Groupware",
    "Collaboration",
    "CRM",
    "Calendar",
    "Addressbook",
    "Teamwork",
    "Tasks",
    "Timetracking"
  ],
  "homepage": "https://www.tine-groupware.de/",
  "license": [
    "AGPL-3.0",
    "BSD-3-Clause",
    "LGPL-2.1+",
    "MIT"
  ],
