{% block manageConsentPageExplainText %}
    {% if contact != '' %}
        <h1>{{ 'Einwilligung von' }}: <span>{{ contact.email }}</span></h1>
        <div class="my-4">
            {% if contact.n_fileas != '' %}<a>Name: {{ contact.n_fileas }}</a><br>{% endif %}
            {% if contact.org_name != '' %}<a>Unternehmen: {{ contact.org_name }}</a>{% endif %}
        </div>
    {% endif %}
        <div class="my-4">
            <a><b>{{ 'Willkommen bei Ihrer Einwilligungsverwaltung.'}}</b></a><br>
            <a>{{ 'Hier können Sie festlegen, zu welchen Zwecken wir Sie kontaktieren dürfen.'}}</a>
        </div>
{% endblock %}
